{
  "compilerOptions": {
    "composite": true,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "noLib": false,
    "sourceMap": false,
    "noImplicitAny": true,
    "strict": true,
    "strictNullChecks": true
  }
}
